{
    "name": "infinety/crud_mod",
    "description": "Quickly build an admin interface for your Eloquent models, using Laravel 5. Build a CMS in a matter of minutes.",
    "keywords": [
        "CRUD",
        "create",
        "update",
        "delete",
        "read",
        "infinety",
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Infinety",
            "email": "hello@infinety.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "prologue/alerts": "0.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Infinety\\CRUD\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Infinety\\CRUD\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
