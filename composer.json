{
    "name": "infinety-es/crud",
    "description": "Quickly build an admin interface for your Eloquent models, using Laravel 5. Build a CMS in a matter of minutes.",
    "keywords": [
        "CRUD",
        "create",
        "update",
        "delete",
        "read",
        "infinety"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Infinety",
            "email": "hello@infinety.com"
        }
    ],
    "require": {
        "illuminate/support": "5.*",
        "php" : ">=7.1.0",
        "jenssegers/date" :"^3.2",
        "laravelcollective/html" : "5.7.*",
        "yajra/laravel-datatables-oracle" : "8.*",
        "jorgejavierleon/laravelpnotify" : "~1.0",
        "infinety-es/filemanager": "2.*",
        "spatie/laravel-medialibrary": "7.*",
        "dimsav/laravel-translatable": "9.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Infinety\\CRUD\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infinety\\CRUD\\CrudServiceProvider"
            ],
            "aliases": {
                "CRUD": "Infinety\\CRUD\\CrudServiceProvider"
            }
        }
    }
}
